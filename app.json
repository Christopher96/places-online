{
    "expo": {
        "name": "places-online",
        "slug": "places-online",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "places-online",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.niftygames.places.online",
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "config": {
                "googleMaps": {
                    "apiKey": "AIzaSyDiHkaLSH1xNd8_MQ7lhf6V7SpI1SZLH8I"
                }
            },
            "permissions": [
                "ACCESS_FINE_LOCATION",
                "LOCATION"
            ]
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        }
    }
}
